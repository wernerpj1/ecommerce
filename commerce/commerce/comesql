INSERT INTO "ecomm_category" VALUES ('Patchwork','patchwork');
INSERT INTO "ecomm_category" VALUES ('Peças e Acessórios','pecas_e_acessorios');
INSERT INTO "ecomm_marca" VALUES (1,'Singer','singer');
INSERT INTO "ecomm_marca" VALUES (2,'Jenome','jenome');
INSERT INTO "ecomm_marca" VALUES (3,'Brother','brother');
INSERT INTO "ecomm_marca" VALUES (4,'Diversas','diversas');
