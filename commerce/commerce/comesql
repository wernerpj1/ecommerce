BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS "ecomm_category" (
	"name"	varchar(255) NOT NULL,
	"slug"	varchar(50) NOT NULL,
	PRIMARY KEY("name")
);
CREATE TABLE IF NOT EXISTS "ecomm_marca" (
	"id"	integer NOT NULL,
	"name"	varchar(30),
	"slug"	varchar(50) NOT NULL,
	PRIMARY KEY("id" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "ecomm_product" (
	"id"	integer NOT NULL,
	"tipo"	varchar(20),
	"ref"	varchar(20),
	"nome"	varchar(200),
	"publicado"	bool,
	"em_destaque"	bool,
	"visibilidade_no_catálogo"	varchar(20),
	"descricao_breve"	varchar(50),
	"descricao"	varchar(500),
	"data_de_inicio_do_preco_promocional"	date,
	"data_de_termino_do_preco_promocional"	date,
	"situacao_fiscal"	varchar(15),
	"classe_de_imposto"	varchar(20),
	"em_stock"	bool,
	"stock"	decimal,
	"valor_de_stock_baixo"	decimal,
	"permitir_encomendas_pendentes"	bool NOT NULL,
	"vender_individualmente"	bool NOT NULL,
	"peso"	decimal,
	"comprimento"	decimal,
	"largura"	decimal,
	"altura"	decimal,
	"permitir_avaliacoes_de_clientes"	bool NOT NULL,
	"preco_promocional"	decimal,
	"preco_normal"	decimal,
	"etiquetas"	varchar(100),
	"classes_de_envio"	varchar(20),
	"imagens"	varchar(100),
	"limite_de_descarregamento"	decimal,
	"dias_para_expirar_o_descarregamento"	decimal,
	"pai"	varchar(20),
	"produtos_agrupados"	varchar(50),
	"aumenta_vendas"	bool,
	"vendas_cruzadas"	bool,
	"url_externo"	varchar(50) NOT NULL,
	"texto_do_botao"	varchar(15),
	"posicao"	varchar(20),
	"categorias_id"	varchar(255) NOT NULL,
	"nota_da_encomenda"	varchar(100),
	"thumbnail"	varchar(100),
	"marca_id"	bigint NOT NULL,
	FOREIGN KEY("categorias_id") REFERENCES "ecomm_category"("name") DEFERRABLE INITIALLY DEFERRED,
	PRIMARY KEY("id" AUTOINCREMENT),
	FOREIGN KEY("marca_id") REFERENCES "ecomm_marca"("id") DEFERRABLE INITIALLY DEFERRED
);
CREATE TABLE IF NOT EXISTS "assistencia_ordemservico" (
	"id"	integer NOT NULL,
	"first_name"	varchar(100) NOT NULL,
	"last_name"	varchar(100) NOT NULL,
	"email"	varchar(100) NOT NULL UNIQUE,
	"address"	varchar(100) NOT NULL,
	"zipcode"	varchar(100) NOT NULL,
	"place"	varchar(100) NOT NULL,
	"phone"	varchar(100) NOT NULL,
	"created_at"	datetime NOT NULL,
	"product_id"	bigint NOT NULL,
	"user_id"	integer NOT NULL,
	PRIMARY KEY("id" AUTOINCREMENT),
	FOREIGN KEY("user_id") REFERENCES "auth_user"("id") DEFERRABLE INITIALLY DEFERRED,
	FOREIGN KEY("product_id") REFERENCES "ecomm_product"("id") DEFERRABLE INITIALLY DEFERRED
);
INSERT INTO "ecomm_category" VALUES ('Domesticas','domesticas');
INSERT INTO "ecomm_category" VALUES ('Industriais','industriais');
INSERT INTO "ecomm_category" VALUES ('Bordadeiras','bordadeiras');
